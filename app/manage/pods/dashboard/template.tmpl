<div class="dashboard">
  <div class="dashboard-content">
    <dashboard-cell icon="/manage/img/dashboard-devices.svg" loader-if="!devices">
      {{devices.length}} Devices
    </dashboard-cell>
    <dashboard-cell icon="/manage/img/dashboard-locations.svg" loader-if="!locations">
      {{locations.length}} Locations
    </dashboard-cell>
    <dashboard-cell icon="/manage/img/dashboard-alerts.svg" loader-if="!devices">&mdash; Alerts</dashboard-cell>
  </div>
</div>
